{
  "name": "yetty",
  "version": "1.0.0",
  "description": "Yet another starter repository for a website with optional blog using the Eleventy static site generator.",
  "scripts": {
    "build": "npm run sass && eleventy && postcss dist/style/main.css --replace",
    "dev": "concurrently 'eleventy --serve' 'npm run sass:watch'",
    "start": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "sass": "node-sass src/_includes/inline_style/scss/ --output src/_includes/inline_style/ --output-style compressed && node-sass src/style/scss/ --output src/style/  --output-style compressed",
    "sass:watch": "npm run sass && npm run sass -- -w",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ygoex/yetty.git"
  },
  "author": {
    "name": "Yolanda Gorriz Exposito",
    "email": "helloygoex@gmail.com",
    "url": "https://ygoex.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ygoex/yetty/issues"
  },
  "homepage": "https://github.com/ygoex/yetty#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.5.1",
    "clean-css": "latest",
    "concurrently": "^6.0.0",
    "critical": "^2.0.0-23",
    "cross-env": "^7.0.3",
    "eleventy-critical-css": "^0.2.3",
    "html-minifier": "^4.0.0",
    "luxon": "^1.25.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "node-sass": "^5.0.0",
    "postcss": "^8.1.0",
    "postcss-cli": "^8.3.1",
    "rimraf": "^3.0.2"
  }
}
