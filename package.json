{
  "name": "eleventy-base-blog",
  "version": "5.0.2",
  "description": "A starter repository for a blog web site using the Eleventy static site generator.",
  "scripts": {
    "build": "npm run sass && eleventy && postcss dist/style/main.css --replace",
    "watch": "eleventy --watch",
    "serve": "concurrently 'eleventy --serve' 'npm run sass:watch'",
    "dev": "npm run build && eleventy --serve",
    "start": "eleventy --serve",
    "debug": "DEBUG=* eleventy",
    "sass": "node-sass src/_includes/critical_style/scss/ --output src/_includes/critical_style/ --output-style compressed && node-sass src/style/scss/ --output src/style/  --output-style compressed",
    "sass:watch": "npm run sass && npm run sass -- -w",
    "clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/11ty/eleventy-base-blog.git"
  },
  "author": {
    "name": "Zach Leatherman",
    "email": "zachleatherman@gmail.com",
    "url": "https://zachleat.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/11ty/eleventy-base-blog/issues"
  },
  "homepage": "https://github.com/11ty/eleventy-base-blog#readme",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.1",
    "@11ty/eleventy-navigation": "^0.1.6",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^3.0.6",
    "@fullhuman/postcss-purgecss": "^1.2.0",
    "autoprefixer": "^9.5.1",
    "concurrently": "^6.0.0",
    "critical": "^2.0.0-23",
    "luxon": "^1.25.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.5",
    "node-sass": "^5.0.0",
    "postcss": "^8.1.0",
    "postcss-cli": "^8.3.1",
    "clean-css": "latest"
  }
}
